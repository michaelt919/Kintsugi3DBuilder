<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="100.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tetzlaff.ibrelight.javafx.controllers.menubar.UnzipFileSelectionController">
    <Label layoutX="14.0" layoutY="14.0" text="Metashape File: " />
    <TextField fx:id="psxPathTxtField" layoutX="130.0" layoutY="10.0" prefHeight="25.0" prefWidth="310.0" />
    <Button layoutX="450.0" layoutY="10.0" mnemonicParsing="false" onAction="#selectPSX" text="Choose .psx file..." />
    <Button fx:id="runButton" layoutX="11.0" layoutY="70.0" mnemonicParsing="false" onAction="#unzip" text="Run" />
</AnchorPane>