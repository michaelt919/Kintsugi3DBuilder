               <ToggleGroup fx:id="LeftPanelNav" />
            </toggleGroup></RadioButton>
         <RadioButton fx:id="TexturesButton" maxHeight="32.0" maxWidth="1.7976931348623157E308" minHeight="32.0" mnemonicParsing="false" onAction="#chooseTabAction" prefHeight="32.0" style="-fx-alignment: center;" styleClass="stripped-radio-button" text="Textures" textAlignment="CENTER" toggleGroup="$LeftPanelNav" HBox.hgrow="ALWAYS">
            <HBox.margin>
               <Insets />
            </HBox.margin>
            <opaqueInsets>
               <Insets />
            </opaqueInsets></RadioButton>
      </children>
   </HBox>
    <fx:include fx:id="cameraTab" maxWidth="500.0" source="CameraTab.fxml" />
    <fx:include fx:id="textureTab" maxWidth="500.0" source="TextureTab.fxml" visible="false" managed="false"/>
</VBox>
