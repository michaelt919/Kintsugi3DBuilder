<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id = "outerGridPane" alignment="CENTER" styleClass="grid-pane" stylesheets="@../kintsugiStyling.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kintsugi3d.builder.javafx.controllers.menubar.fxmlpageutils.FXMLPageScrollerController">
   <columnConstraints>
       <ColumnConstraints prefWidth="200.0" />
       <ColumnConstraints prefWidth="200.0" />
       <ColumnConstraints prefWidth="200.0" />
   </columnConstraints>
   <rowConstraints>
       <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <AnchorPane fx:id="hostAnchorPane" prefHeight="334.0" prefWidth="600.0" GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
      <Button fx:id="prevButton" mnemonicParsing="false" onAction="#prevPage" text="Back" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="nextButton" mnemonicParsing="false" onAction="#nextPage" text="Next" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </GridPane.margin>
      </Button>
   </children>
</GridPane>
