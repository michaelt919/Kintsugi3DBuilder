<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="100.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="tetzlaff.ibrelight.javafx.controllers.menubar.ColorCheckerController">
    <Label layoutX="14.0" layoutY="14.0" text="Color Checker Image: " />
    <TextField fx:id="colorPickerSetField" layoutX="130.0" layoutY="10.0" prefHeight="25.0" prefWidth="310.0" />
    <Button layoutX="450.0" layoutY="10.0" mnemonicParsing="false" text="Choose image..." />
    <Button fx:id="runButton" layoutX="11.0" layoutY="70.0" mnemonicParsing="false" text="Run" />
   <ImageView fx:id="colorPickerImgView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" translateX="200.0" />
</AnchorPane>
