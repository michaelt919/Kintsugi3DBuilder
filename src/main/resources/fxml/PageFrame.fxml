<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="outerGridPane" alignment="CENTER" styleClass="modal-root" stylesheets="@kintsugiStyling.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kintsugi3d.builder.javafx.controllers.paged.PageFrameController">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <StackPane fx:id="hostPane" minWidth="800.0" GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
    <Button fx:id="prevButton" mnemonicParsing="false" onAction="#prevPage" styleClass="wireframeBodyLarge, button" text="Back" GridPane.rowIndex="1">
        <GridPane.margin>
          <Insets />
        </GridPane.margin>
    </Button>
    <Button fx:id="nextButton" mnemonicParsing="false" onAction="#nextPage" styleClass="wireframeBodyLarge, button" text="Next" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
        <GridPane.margin>
          <Insets />
        </GridPane.margin>
    </Button>
</GridPane>
