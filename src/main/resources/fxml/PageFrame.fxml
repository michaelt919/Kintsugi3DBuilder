<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Line?>
<VBox fx:id="outerRoot" alignment="CENTER" styleClass="modal-root" stylesheets="@kintsugiStyling.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kintsugi3d.builder.javafx.controllers.paged.PageFrameController">
    <StackPane fx:id="hostPane" minWidth="924.0" />
     <Line endX="924.0" fill="#cecece" stroke="#cecece" strokeWidth="2.0">
         <VBox.margin>
             <Insets top="80.0" />
         </VBox.margin>
     </Line>
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" />
            <ColumnConstraints hgrow="SOMETIMES" />
            <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Button fx:id="prevButton" mnemonicParsing="false" onAction="#prevPage" styleClass="wireframeBodyLarge" text="Back" GridPane.rowIndex="0">
            <GridPane.margin>
              <Insets />
            </GridPane.margin>
        </Button>
        <Button fx:id="nextButton" mnemonicParsing="false" onAction="#advancePage" styleClass="wireframeBodyLarge" text="Next" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="0">
            <GridPane.margin>
              <Insets />
            </GridPane.margin>
        </Button>
      <VBox.margin>
         <Insets top="36.0" />
      </VBox.margin>
    </GridPane>
</VBox>
