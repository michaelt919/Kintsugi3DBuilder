<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Line?>
<VBox prefWidth="924.0" styleClass="content-root" stylesheets="@kintsugiStyling.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kintsugi3d.builder.javafx.core.ProgressBarsController">
   <GridPane.margin>
      <Insets left="24.0" top="48.0" />
   </GridPane.margin>
   <Label fx:id="overallTextLabel" style="-fx-text-fill: #CECECE;" styleClass="wireframeTitle" text="Loading..." wrapText="true">
      <padding>
         <Insets right="20.0" />
      </padding>
   </Label>
   <VBox maxWidth="1.7976931348623157E308">
      <VBox.margin>
         <Insets left="16.0" right="16.0" top="16.0" />
      </VBox.margin>
      <Label alignment="TOP_LEFT" style="-fx-text-fill: #CECECE;" styleClass="wireframeSubtitle" text="Overall Progress">
      </Label>
      <ProgressBar fx:id="overallProgressBar" maxWidth="1.7976931348623157E308" prefHeight="40.0" progress="0.0">
         <VBox.margin>
            <Insets left="12.0" right="12.0" top="16.0" />
         </VBox.margin>
      </ProgressBar>
      <Label fx:id="totalElapsedTimeLabel" style="-fx-text-fill: #CECECE;" styleClass="wireframeBodyLarge" text="--:--:-- Lapsed">
         <VBox.margin>
            <Insets left="20.0" top="8.0" />
         </VBox.margin>
      </Label>
   </VBox>
   <VBox maxWidth="1.7976931348623157E308">
      <VBox.margin>
         <Insets left="28.0" right="28.0" top="48.0" />
      </VBox.margin>
      <Label fx:id="localTextLabel" style="-fx-text-fill: #CECECE;" styleClass="wireframeBodyLarge" text="Please wait...">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <ProgressBar fx:id="localProgressBar" maxWidth="1.7976931348623157E308" prefHeight="24.0" prefWidth="582.0" progress="0.0">
         <VBox.margin>
            <Insets left="20.0" right="20.0" top="16.0" />
         </VBox.margin>
      </ProgressBar>
      <HBox spacing="2.0">
         <padding>
            <Insets left="32.0" />
         </padding>
         <VBox.margin>
            <Insets top="8.0" />
         </VBox.margin>
         <Label fx:id="localEstimTimeRemainingLabel" style="-fx-text-fill: #CECECE;" styleClass="wireframeCaption" text="Calculating remaining time... " />
         <Label fx:id="localElapsedTimeLabel" style="-fx-text-fill: #CECECE;" styleClass="wireframeCaption" text="(--:--:-- Lapsed)" />
      </HBox>
   </VBox>
   <Line endX="876.0" fill="#cecece" stroke="#cecece" strokeWidth="2.0">
      <VBox.margin>
         <Insets top="80.0" />
      </VBox.margin>
   </Line>
   <GridPane>
      <VBox.margin>
         <Insets top="36.0" />
      </VBox.margin>
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="458.0" minWidth="10.0" />
         <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="751.0" minWidth="10.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="751.0" minWidth="10.0" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <Button fx:id="cancelButton" alignment="CENTER" mnemonicParsing="false" prefHeight="40.0" styleClass="wireframeBodyLarge" text="Cancel">
         <padding>
            <Insets left="30.0" right="30.0" />
         </padding>
      </Button>
      <Label fx:id="pageCountLabel" alignment="CENTER" text="4/5" visible="false" GridPane.columnIndex="1" />
      <Button fx:id="doneButton" alignment="CENTER" mnemonicParsing="false" prefHeight="40.0" styleClass="wireframeBodyLarge" text="Done" GridPane.columnIndex="2" GridPane.halignment="RIGHT" />
   </GridPane>
</VBox>
