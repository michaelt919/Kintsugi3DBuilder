<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane styleClass="modal-root" prefHeight="178.0" prefWidth="331.0" stylesheets="@../../kintsugiStyling.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kintsugi3d.builder.javafx.controllers.modals.systemsettings.PhotoLightingSettingsController">
    <VBox spacing="10.0">
        <padding>
            <Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
        </padding>
        <CheckBox fx:id="fresnelEffectCheckBox" mnemonicParsing="false" text="Fresnel effect" />
        <CheckBox fx:id="shadowsCheckBox" mnemonicParsing="false" selected="true" text="Shadows" />
        <CheckBox fx:id="phyMaskingCheckBox" mnemonicParsing="false" text="Physically based masking/shadowing" />
        <CheckBox fx:id="relightingCheckBox" mnemonicParsing="false" selected="true" text="Relighting" />
        <CheckBox fx:id="visibleLightWidgetsCheckBox" mnemonicParsing="false" selected="true" text="Show light widgets" />
    </VBox>
</AnchorPane>
