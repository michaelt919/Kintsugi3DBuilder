<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="rootPane" alignment="TOP_CENTER" spacing="32.0" styleClass="root" stylesheets="@../../kintsugiStyling.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kintsugi3d.builder.javafx.controllers.modals.createnewproject.SelectImportOptionsController">
   <Label alignment="CENTER" styleClass="wireframeTitle" text="Select Import Option" textAlignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
   <ToggleButton fx:id="metashapeImportButton" mnemonicParsing="false" prefWidth="330.0" styleClass="wireframeSubtitle" text="Metashape">
      <padding>
         <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
      </padding></ToggleButton>
   <ToggleButton fx:id="looseFilesImportButton" mnemonicParsing="false" prefWidth="330.0" styleClass="wireframeSubtitle" text="Loose Files">
      <padding>
         <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
      </padding></ToggleButton>
   <ToggleButton fx:id="realityCaptureImportButton" layoutX="150.0" layoutY="240.0" mnemonicParsing="false" prefWidth="330.0" styleClass="wireframeSubtitle" text="Reality Capture">
      <padding>
         <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
      </padding></ToggleButton>
</VBox>
