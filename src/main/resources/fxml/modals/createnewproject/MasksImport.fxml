<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox fx:id="rootPane" alignment="TOP_CENTER" spacing="32.0" styleClass="root" stylesheets="@../../kintsugiStyling.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kintsugi3d.builder.javafx.controllers.modals.createnewproject.MasksImportController">
   <Label alignment="CENTER" styleClass="wireframeTitle" text="Select Masks Import Option" textAlignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
   <ToggleButton fx:id="useProjectMasksButton" mnemonicParsing="false" prefWidth="330.0" styleClass="wireframeSubtitle" text="Use Project Masks">
      <padding>
         <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
      </padding>
   </ToggleButton>
   <ToggleButton fx:id="customMasksDirButton" mnemonicParsing="false" prefWidth="330.0" styleClass="wireframeSubtitle" text="Choose New Masks Directory">
      <padding>
         <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
      </padding>
   </ToggleButton>
   <ToggleButton fx:id="noMasksButton" layoutX="150.0" layoutY="240.0" mnemonicParsing="false" prefWidth="330.0" styleClass="wireframeSubtitle" text="Do Not Use Masks">
      <padding>
         <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
      </padding>
   </ToggleButton>
</VBox>
