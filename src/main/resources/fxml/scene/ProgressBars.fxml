<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="anchor-pane" stylesheets="@../kintsugiStyling.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kintsugi3d.builder.javafx.controllers.scene.ProgressBarsController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" spacing="30.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="overallTextLabel" text="Loading..." wrapText="true">
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            <VBox spacing="10.0">
               <children>
                  <Label text="Overall Progress">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <ProgressBar fx:id="overallProgressBar" prefHeight="18.0" prefWidth="582.0" progress="0.0" styleClass="overall-progress-bar" />
                  <HBox>
                     <children>
                        <Label text="Total Time Elapsed: " />
                        <Label fx:id="totalElapsedTimeLabel" text="N/A" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label text="Estimated Time Remaining: " />
                        <Label fx:id="totalEstimTimeRemainingLabel" text="N/A" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox spacing="10.0">
               <children>
                  <Label fx:id="localTextLabel" text="..." />
                  <ProgressBar fx:id="localProgressBar" prefHeight="18.0" prefWidth="582.0" progress="0.0" />
                  <HBox>
                     <children>
                        <Label text="Time Elapsed: " />
                        <Label fx:id="localElapsedTimeLabel" text="N/A" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label text="Estimated Time Remaining: " />
                        <Label fx:id="localEstimTimeRemainingLabel" text="N/A" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <HBox alignment="CENTER">
               <children>
                  <Button fx:id="cancelButton" alignment="CENTER" mnemonicParsing="false" text="Cancel" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
