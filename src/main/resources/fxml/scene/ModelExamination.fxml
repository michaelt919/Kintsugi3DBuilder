        <fx:include fx:id="framebufferView" source="../menubar/FramebufferView.fxml"/>
    </HBox>
   <Button fx:id="continueButton" mnemonicParsing="false" onAction="#openObjectOrientation" text="Continue" AnchorPane.bottomAnchor="30.0" AnchorPane.rightAnchor="30.0" />
</AnchorPane>
